    "exclude": [
      "**/*.spec.{js,ts}",
      "**/*.test.{js,ts,jsx,tsx}"
    ]
  },
  "simple-git-hooks": {
    "pre-push": "./scripts/pre-push.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
